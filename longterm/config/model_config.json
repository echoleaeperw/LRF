{
  "default_provider": "deepseek",
  "providers": {
    "openai": {
      "model_name": "gpt-4",
      "temperature": 0.0,
      "max_tokens": 4000,
      "request_timeout": 60,
      "streaming": true,
      "api_key_env": "OPENAI_API_KEY"
    },
    "deepseek": {
      "model_name": "deepseek-reasoner",
      "temperature": 0.0,
      "max_tokens": 4000,
      "request_timeout": 60,
      "streaming": true,
      "api_key_env": "DEEPSEEK_API_KEY",
      "base_url": "https://api.deepseek.com/v1"
    },
    "azure": {
      "deployment_name_env": "AZURE_CHAT_DEPLOY_NAME",
      "temperature": 0.0,
      "max_tokens": 4000,
      "request_timeout": 60,
      "streaming": true,
      "api_key_env": "OPENAI_API_KEY"
    }
  },
  "callbacks": {
    "verbose": false,
    "streaming_stdout": true
  }
} 